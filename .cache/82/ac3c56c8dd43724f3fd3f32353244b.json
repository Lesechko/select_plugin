<li class=\"select__item\">Angular</li>\n\t\t\t\t<li class=\"select__item\">React</li>\n\t\t\t\t<li class=\"select__item\">Vue</li>\n\t\t\t\t<li class=\"select__item\">Angular</li>\n\t\t\t</ul>\n\t\t</div>\n        `;\n\t}\n\n\tdestroy() {\n\t\tthis.$select.removeEventListener('click', this.clickHandler);\n\t\tdocument.body.removeEventListener('click', this.clickHandler);\n\t}\n}\n"},"lineCount":null}},"error":null,"hash":"ed477335d3c251083b56642ff20b1437","cacheData":{"env":{}}}